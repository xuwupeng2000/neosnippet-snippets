snippet log     
abbr console.log        
  console.log(${0})

snippet     li
abbr        <li>
  %li

snippet     i
abbr        <i>
  %i

snippet     a
abbr        <a>
  %a

snippet     rp
abbr        render partial
  = render partial: "${0}"

snippet     modal
abbr        modal
  .modal.fade#${0}
    .modal-dialog
      .modal-content
        .modal-header
        .modal-body
        .modal-footer
